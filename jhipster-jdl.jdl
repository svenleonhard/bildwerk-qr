entity QrRoute {
	description String
    code String
    url String
    enabled Boolean
    startDate LocalDate
    endDate LocalDate
}

entity UserQrCode {
  code String
}

entity UserQrCodeExposed {
  code String
}

relationship ManyToOne {
	QrRoute{user} to User
}

relationship OneToOne {
   UserQrCode{user} to User
}

// Set pagination options
paginate QrRoute with infinite-scroll
service all with serviceImpl
filter all